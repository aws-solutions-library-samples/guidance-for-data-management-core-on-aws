{
	"name": "@df/resource-api-base",
	"version": "1.0.0",
	"description": "Base containing common functionality to support all resource API's.",
	"author": "Dean Hart <deanhart@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.js",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "tsc -b",
		"test": "npm run build && vitest run"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "~3",
		"@aws-sdk/client-lambda": "~3",
		"@aws-sdk/client-sqs": "~3",
		"@aws-sdk/lib-dynamodb": "~3",
		"@aws-sdk/util-dynamodb": "~3",
		"@df/lambda-invoker": "workspace:*",
		"@fastify/awilix": "~5",
		"@fastify/type-provider-typebox": "~4",
		"@sinclair/typebox": "~0",
		"awilix": "~10",
		"aws-xray-sdk": "~3",
		"fastify": "~4",
		"pino": "~8"
	},
	"devDependencies": {
		"@tsconfig/node20": "~20",
		"@types/node": "~22",
		"@typescript-eslint/eslint-plugin": "~8",
		"@typescript-eslint/parser": "~8",
		"aws-sdk-client-mock": "~4",
		"eslint": "~8",
		"shx": "~0",
		"tsx": "~4",
		"typescript": "~5",
		"vitest": "~2",
		"vitest-mock-extended": "~2"
	},
	"private": false
}

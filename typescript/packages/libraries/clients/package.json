	"dependencies": {
		"@df/events": "workspace:*",
		"axios": "~1"
	},
	"devDependencies": {
		"@tsconfig/node20": "~20",
		"@types/node": "~22",
		"@typescript-eslint/eslint-plugin": "~8",
		"@typescript-eslint/parser": "~8",
		"aws-sdk-client-mock": "~4",
		"eslint": "~8",
		"pino-pretty": "~10",
		"pino": "~8",
		"shx": "~0",
		"tsx": "~4",
		"typescript": "~5",
		"vitest-mock-extended": "~2",
		"vitest": "~2"
	},
	"private": false

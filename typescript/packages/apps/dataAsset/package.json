		"@aws-sdk/client-s3": "~3",
		"@aws-sdk/client-secrets-manager": "~3",
		"@aws-sdk/client-sfn":  "~3",
		"@aws-sdk/client-ssm":  "~3",
		"@aws-sdk/client-sts":  "~3",
		"@aws-sdk/lib-dynamodb": "~3",
		"@aws-sdk/s3-request-presigner" : "~3",
		"@aws-sdk/types" : "~3",
		"@df/cdk-common": "workspace:*",
		"@df/dynamodb-utils": "workspace:*",
		"@df/events": "workspace:*",
